{
  "name": "@green-systems/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "ng": "nx",
    "serve": "nx serve",
    "start": "nx serve",
    "start:topstack": "nx serve topstack",
    "script": "ts-node -r tsconfig-paths/register --project scripts/tsconfig.json",
    "dev": "nx serve topstack",
    "build": "nx build",
    "build:all": "nx run-many -t build --parallel=5 --configuration=production",
    "build:topstack": "nx build topstack --configuration=production",
    "lint": "nx lint",
    "ci": "nx affected -t lint test build --base=origin/main --head=HEAD",
    "lint:all": "nx run-many -t lint && stylelint -- **/*.scss",
    "lint:style": "stylelint -- **/*.scss",
    "lint:style:fix": "stylelint -- **/*.scss --fix",
    "test": "nx test",
    "test:all": "nx run-many -t test",
    "test:affected": "nx affected -t lint --base=main --head=HEAD --parallel=5 && stylelint -- **/*.scss --fix",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:gen-types": "supabase gen types typescript --local > supabase/types/supabase.ts",
    "update": "nx migrate latest && npm install && npx nx migrate --run-migrations",
    "reset": "nx reset"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.2.0",
